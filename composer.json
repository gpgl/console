{
    "name": "gpgl/console",
    "description": "Console application for GPL PHP GPG Locker.",
    "license": "GPL-3.0+",
    "authors": [
        {
            "name": "Jeff Puckett",
            "email": "jeff@jeffpuckett.com"
        }
    ],
    "bin": ["bin/gpgl"],
    "require": {
        "symfony/console": "^3.2",
        "gpgl/core": "dev-dev"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../core",
            "options": {
                "symlink": false
            }
        }
    ],
    "autoload": {
        "psr-4": {
            "gpgl\\console\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1"
    }
}
